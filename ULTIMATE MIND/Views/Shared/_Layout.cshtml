<!DOCTYPE html>
<html lang="en">
<head>
    <environment>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>@ViewData["Title"] - ULTIMATE MIND</title>
        <link rel="stylesheet" href="/frameworks/bootstrap-5.3.0/css/bootstrap.min.css?v=@ViewBag.Versao">
        <link rel="stylesheet" href="/frameworks/Select2/select2.min.css?v=@ViewBag.Versao" />
        <link rel="stylesheet" href="/frameworks/jquery-confirm/dist/jquery-confirm.min.css?v=@ViewBag.Versao" />
        <link rel="stylesheet" href="~/Font-Awesome/css/font-awesome.min.css?v=@ViewBag.Versao" />
        <link rel="stylesheet" href="~/css/site.css?v=@ViewBag.Versao" />

        <script src="/frameworks/JQuery/jQuery.min.js?v=@ViewBag.Versao"></script>
        <script src="/frameworks/Select2/select2.min.js?v=@ViewBag.Versao"></script>
        <script src="/frameworks/jquery-confirm/dist/jquery-confirm.min.js" ?v=@ViewBag.Versao></script>
        <script src="~/frameworks/jQuery-mask/jquery.mask.min.js?v=@ViewBag.Versao"></script>
        <script type="text/javascript" src="~/frameworks/bootstrap-5.3.0/popper.js?v=@ViewBag.Versao"></script>
        <script type="text/javascript" src="~/frameworks/bootstrap-5.3.0/js/bootstrap.bundle.min.js?v=@ViewBag.Versao"></script>
        <script type="text/javascript" src="~/js/site.js?v=@ViewBag.Versao"></script>
        <script type="text/javascript" src="~/js/siteMenu.js?v=@ViewBag.Versao"></script>

        @RenderSection("sripts", required: false)
    </environment>

</head>

<body class="vh-100">
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark" id="navbar-menu">
        <div class="container-fluid">
            <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">ULTIMATE MIND</a>
            <button class="navbar-toggler ms-auto hamburger-btn" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarCollapse">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0" id="menu-dinamico">

                </ul>
            </div>
        </div>
    </nav>


    <div id="content" style="height: 100vh;">
        @RenderBody()
    </div>
</body>
</html>

<script>
    urlSite = "@ViewBag.UrlSite";
</script>